{

"moduleFileExtensions": ["ts", "tsx", "js"],
  "bail":5,
  "verbose": true,
  "rootDir":"../src",
  "coveragePathIgnorePatterns" :["/node_modules/","/src/classes"],
  "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -50
      }
},
"notify": true
}
