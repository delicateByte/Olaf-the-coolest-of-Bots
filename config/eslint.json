{
  "extends": "airbnb-typescript/base",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "env": {
    "jest": true
  },
  "rules": {
    "linebreak-style": 0,
    "no-console": "off",
    "no-prototype-builtins": "off",
    "lines-between-class-members": ["error", "always", { "exceptAfterSingleLine": true }]
  }
}
